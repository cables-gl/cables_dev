{
  "name": "cables-jsdoc",
  "version": "1.0.0",
  "description": "",
  "main": "build.js",
  "type": "module",
  "scripts": {
    "build": "run-s build:core build:ui build:standalone",
    "build:core": "documentation build ../cables/src/core/index.js --config ../cables/package.json --format html --output dist/ --theme theme/index.js",
    "build:ui": "documentation build ../cables_ui/src/ui/index.js ../shared/client/index.js --config ../cables_ui/package.json  --format html --output dist/ui --theme theme/index.js",
    "build:standalone": "run-s build:standalone_api build:standalone_client",
    "build:standalone_api": "documentation build ../cables_electron/src/electron/main.js ../shared/api/index.js --config ../cables_electron/package.json --format html --output dist/standalone --theme theme/index.js",
    "build:standalone_client": "documentation build ../cables_electron/src_client/index_electron.js ../shared/client/index.js --config ../cables_electron/package.json --format html --output dist/standalone/client --theme theme/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "engines": {
    "node": "20.13.1"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "documentation": "14.0.3",
    "highlight.js": "11.7.0",
    "lodash": "4.17.21",
    "npm-run-all": "4.1.5",
    "github-slugger": "2.0.0"
  }
}
