{
    "name": "cables-jsdoc",
    "version": "1.0.0",
    "description": "",
    "main": "build.js",
    "type": "module",
    "scripts": {
        "build": "run-s build:core build:ui build:standalone build:index",
        "build:dev": "run-s build:devlayout build build:robots",
        "build:core": "esdoc -c config/core.json",
        "build:ui": "esdoc -c config/ui.json",
        "build:standalone": "esdoc -c config/standalone.json",
        "build:devlayout": "cp dev_layout.html template/layout.html",
        "build:robots": "cp dev_robots.txt dist/robots.txt",
        "build:index": "cp index.html dist/index.html",
        "start": "http-server dist/"
    },
    "engines": {
        "node": "20.13.1"
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@enterthenamehere/esdoc": "2.6.0-dev.1",
        "@enterthenamehere/esdoc-importpath-plugin":"2.6.0-dev.1",
        "@enterthenamehere/esdoc-standard-plugin": "2.6.0-dev.2",
        "@enterthenamehere/esdoc-publish-html-plugin": "2.6.0-dev.1",
        "@enterthenamehere/esdoc-inject-style-plugin": "2.6.0-dev.1",
        "@itsjamie/esdoc-importpath-plugin": "0.5.0",
        "npm-run-all": "4.1.5",
        "http-server": "14.1.1"
    }
}
