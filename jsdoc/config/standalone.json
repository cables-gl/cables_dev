{
  "index": "../cables_electron/README.md",
  "source": "../",
  "destination": "dist/standalone",
  "includes": [
    "^cables_electron/src/.*\\.js$",
    "^cables_electron/src_client/.*\\.js$",
    "^shared/client/index.js$",
    "^shared/api/index.js$"
  ],
  "excludes": [
    "node_modules"
  ],
  "plugins": [
      {
          "name": "@enterthenamehere/esdoc-standard-plugin"
      },
      {
          "name": "@enterthenamehere/esdoc-brand-plugin",
          "option": {
              "title": "cables.gl - standalone",
              "repository": "https://github.com/cables-gl/cables_electron",
              "site": "https://cables.gl",
              "author": "https://undev.studio"
          }
      },
      {
          "name": "@enterthenamehere/esdoc-inject-style-plugin",
          "option": {
              "enable": true,
              "styles": [
                  "./cables.css"
              ]
          }
      },
      {
          "name": "@enterthenamehere/esdoc-inject-style-plugin",
          "option": {
              "enable": true,
              "styles": [
                  "./cables.css"
              ]
          }
      },
      {
          "name": "@enterthenamehere/esdoc-importpath-plugin",
          "option": {
              "stripPackageName": true,
              "replaces": [
                  {
                      "from": "^cables_dev/cables_electron/src/",
                      "to": "./"
                  },
                  {
                      "from": "^cables_dev/cables_electron/src_client/",
                      "to": "./"
                  },
                  {
                      "from": "^cables_dev/shared/client/index.js",
                      "to": "cables-shared-client"
                  },
                  {
                      "from": "^cables_dev/shared/api/index.js",
                      "to": "cables-shared-api"
                  }
              ]
          }
      }
  ]
}
