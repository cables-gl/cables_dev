{
    "index": "../cables/README.md",
    "source": "../",
    "destination": "dist/cables",
    "includes": [
        "^cables/src/core/.*\\.js$",
        "^cables/src/libs/.*\\.js$"
    ],
    "excludes": [
        "node_modules"
    ],
    "plugins": [
        {
            "name": "@enterthenamehere/esdoc-standard-plugin"
        },
        {
            "name": "@enterthenamehere/esdoc-brand-plugin",
            "option": {
                "title": "cables.gl",
                "repository": "https://github.com/cables-gl/cables",
                "site": "https://cables.gl",
                "author": "https://undev.studio"
            }
        },
        {
            "name": "@enterthenamehere/esdoc-inject-style-plugin",
            "option": {
                "enable": true,
                "styles": [
                    "./cables.css"
                ]
            }
        },
        {
            "name": "@enterthenamehere/esdoc-importpath-plugin",
            "option": {
                "stripPackageName": true,
                "replaces": [
                    {
                        "from": "^cables_dev/cables/src/",
                        "to": "./"
                    }
                ]
            }
        }
    ]
}
