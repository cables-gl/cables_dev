{
    "index": "../cables_ui/README.md",
    "source": "../",
    "destination": "dist/ui",
    "includes": [
        "^cables_ui/src/.*\\.js$",
        "^shared/client/index.js$"
    ],
    "excludes": [
        "node_modules"
    ],
    "plugins": [
        {
            "name": "@enterthenamehere/esdoc-standard-plugin"
        },
        {
            "name": "@enterthenamehere/esdoc-type-inference-plugin",
            "option": {
                "enable": true
            }
        },
        {
            "name": "@enterthenamehere/esdoc-brand-plugin",
            "option": {
                "title": "cables.gl - editor",
                "repository": "https://github.com/cables-gl/cables_ui",
                "site": "https://cables.gl",
                "author": "https://undev.studio"
            }
        },
        {
            "name": "@enterthenamehere/esdoc-inject-style-plugin",
            "option": {
                "enable": true,
                "styles": [
                    "./cables.css"
                ]
            }
        },
        {
            "name": "@enterthenamehere/esdoc-importpath-plugin",
            "option": {
                "stripPackageName": true,
                "replaces": [
                    {
                        "from": "^cables_dev/cables_ui/src/",
                        "to": "./"
                    },
                    {
                        "from": "^cables_dev/shared/client/index.js",
                        "to": "cables-shared-client"
                    }
                ]
            }
        }
    ]
}
