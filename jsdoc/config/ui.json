{
  "index": "../cables_ui/README.md",
  "source": "../",
  "destination": "dist/ui",
  "includes": ["^cables_ui/src/.*\\.js$", "^shared/client/index.js$" ],
  "excludes": ["node_modules"],
  "plugins": [
    {
      "name": "@itsjamie/esdoc-standard-plugin"
    },
    {
      "name": "@itsjamie/esdoc-importpath-plugin",
      "option": {
        "stripPackageName": true,
        "replaces": [
          {"from": "^cables_dev/cables_ui/src/", "to": "./"},
          {"from": "^cables_dev/shared/client/index.js", "to": "cables-shared-client"}
        ]
      }
    }
  ]
}
